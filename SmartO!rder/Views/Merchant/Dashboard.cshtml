@model IEnumerable<SmartO_rder.Models.Order>
<h2>Orders</h2>
<table>
<tr><th>Product</th><th>Quantity</th><th>Date</th></tr>
@foreach(var o in Model){
<tr><td>@o.Product!.Name</td><td>@o.Quantity</td><td>@o.CreatedAt</td></tr>
}
</table>
<a asp-action="AddProduct">Add Product</a> | <a asp-action="AddTable">Add Table</a>
